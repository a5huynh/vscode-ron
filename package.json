{
    "name": "vscode-ron",
    "displayName": "vscode-ron",
    "description": "Rusty Object Notation (RON) syntax package",
    "publisher": "a5huynh",
    "license": "MIT",
    "homepage": "https://github.com/a5huynh/vscode-ron",
    "bugs": {
        "url": "https://github.com/a5huynh/vscode-ron/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/a5huynh/vscode-ron"
    },
    "version": "0.9.0",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "ron",
                "aliases": [
                    "Rusty Object Notation (RON)",
                    "ron"
                ],
                "extensions": [
                    ".ron"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "ron",
                "scopeName": "source.ron",
                "path": "./syntaxes/ron.tmGrammar.json"
            }
        ]
    }
}
